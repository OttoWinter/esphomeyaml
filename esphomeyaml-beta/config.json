{
  "name": "esphomeyaml-beta",
  "version": "1.9.0b2",
  "slug": "esphomeyaml-beta",
  "description": "Beta version of esphomeyaml HassIO add-on.",
  "url": "https://beta.esphomelib.com/esphomeyaml/index.html",
  "startup": "application",
  "webui": "http://[HOST]:[PORT:6052]",
  "boot": "auto",
  "ports": {
    "6052/tcp": 6052,
    "6053/tcp": 6053
  },
  "auto_uart": true,
  "map": [
    "config:rw"
  ],
  "arch": [
    "amd64",
    "armhf",
    "i386"
  ],
  "environment": {
    "ESPHOMEYAML_OTA_HOST_PORT": "6053"
  },
  "options": {
    "password": ""
  },
  "schema": {
    "password": "str?"
  },
  "image": "ottowinter/esphomeyaml-hassio-{arch}"
}
